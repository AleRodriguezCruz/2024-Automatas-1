import json

# Crear un diccionario con los valores Unicode del archivo JSON
unicode_dict = {}
with open('ejemplo.json', 'r', encoding='utf-8') as file:
    data = json.load(file)
    for key, value in data.items():
        unicode_val = [ord(char) for char in value]
        unicode_dict[key] = unicode_val

# Escribir el diccionario en un nuevo archivo JSON
with open('token_final.json', 'w', encoding='utf-8') as file:
    json.dump(unicode_dict, file, ensure_ascii=False, indent=4)
